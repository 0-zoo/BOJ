#include <stdio.h>

int main(void) {
	int sum1 = 0;
	int N = 0;
	int a = 0;
	int b = 0;
	scanf("%d", &sum1);
	scanf("%d", &N);

	int sum2 = 0;
	for (int i = 0; i < N; i++) {
		scanf("%d %d", &a, &b);
		sum2 += a * b;
	}
	if (sum1 == sum2) {
		printf("Yes\n");
	}
	else {
		printf("No\n");
	}
	return 0;
}
